{
  "compilerOptions": {
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@prisma/client": ["node_modules/.prisma/client"]
    },
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true
  }
}
